#emirp number
num = 13
res = 0
copy = num
while num != 0:
    rem = num%10
    res = res*10+rem
    num = num//10
if res != copy:
    for den in range(2,num//2+1):
        if copy%den == 0:
            print('not a prime')
    else:
        for den in range(2,num//2+1):
            if res % den == 0:
                print('not a prime')
        else:
            print ('emirp number')
else:
    print('not an emirp')
